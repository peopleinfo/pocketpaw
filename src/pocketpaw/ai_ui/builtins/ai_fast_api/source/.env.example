# Server settings
HOST=0.0.0.0
PORT=8000
DEBUG=true

# API settings
API_TITLE="AI FastAPI Server"
API_DESCRIPTION="OpenAI-compatible API powered by GPT4Free (G4F)"
API_VERSION=1.0.0

# G4F settings
G4F_PROVIDER=auto
G4F_MODEL=gpt-4o-mini
G4F_TIMEOUT=60
G4F_RETRIES=3

# Rate limiting
RATE_LIMIT_REQUESTS=600
RATE_LIMIT_WINDOW=600

# CORS settings
CORS_ENABLED=true
CORS_ORIGINS="*"

# OpenAI compatibility
OPENAI_API_BASE=/v1

# Sanitizer (defaults: secure â€” redact input, exclude ads)
# SANITIZER_REDACT_INPUT=true   # Redact cards, API keys, passwords from user input
# SANITIZER_EXCLUDE_DOMAINS=    # Comma-separated extra ad domains
# SANITIZER_EXCLUDE_PHRASES=    # Comma-separated extra ad phrases
